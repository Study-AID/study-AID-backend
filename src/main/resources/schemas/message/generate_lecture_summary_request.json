{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://study-aid.dev/schemas/generate-lecture-summary-request.json",
  "title": "GenerateLectureSummaryRequest",
  "type": "object",
  "required": [
    "schema_version",
    "request_id",
    "occurred_at",
    "lecture_id",
    "s3_bucket",
    "s3_key"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
    },
    "request_id": {
      "type": "string",
      "format": "uuid"
    },
    "occurred_at": {
      "type": "string",
      "format": "date-time"
    },
    "lecture_id": {
      "type": "string",
      "format": "uuid"
    },
    "s3_bucket": {
      "type": "string",
      "minLength": 3
    },
    "s3_key": {
      "type": "string",
      "minLength": 1
    },
    "material_type": {
      "type": "string",
      "maxLength": 20
    },
    "course_id": {
      "type": "string",
      "format": "uuid"
    },
    "user_id": {
      "type": "string",
      "format": "uuid"
    },
    "title": {
      "type": "string",
      "maxLength": 255
    }
  },
  "additionalProperties": false
}